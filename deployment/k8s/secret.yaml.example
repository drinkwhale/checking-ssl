apiVersion: v1
kind: Secret
metadata:
  name: ssl-monitoring-secrets
type: Opaque
stringData:
  # PostgreSQL 데이터베이스 연결 문자열
  # 예시: postgresql+asyncpg://user:password@postgres-service:5432/ssl_monitoring
  database-url: "postgresql+asyncpg://user:password@localhost:5432/ssl_monitoring"

  # Microsoft Teams 웹훅 URL (선택사항)
  # 예시: https://outlook.office.com/webhook/xxxxx
  teams-webhook-url: ""
